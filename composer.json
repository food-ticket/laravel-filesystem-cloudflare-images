{
    "name": "foodticket/laravel-filesystem-cloudflare-images",
    "description": "A Laravel filesystem driver for Cloudflare Images",
    "keywords": ["laravel", "Cloudflare", "images", "Cloudflare Images"],
    "type": "library",
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/http": "^8.0|^9.0|^10.0|^11.0",
        "foodticket/laravel-cloudflare": "^v0.0.8"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Foodticket\\FilesystemCloudflareImages\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Rik",
            "email": "rik@foodticket.nl",
            "homepage": "https://foodticket.nl",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Foodticket\\FilesystemCloudflareImages\\FilesystemCloudflareImagesServiceProvider"
            ]
        }
    }
}
